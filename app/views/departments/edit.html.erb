<%= link_to("<< Back to List", departments_path, :class => 'back-link') %>

<div class="departments new">
  <h2>Create Department</h2>

  <%= form_for(@department) do |f| %>

    <table summary="Department form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
      <tr>
    </table>

    <div class="form-buttons">
      <%= f.submit("Update Department") %>
    </div>

  <% end %>
</div>
